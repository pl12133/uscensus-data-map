{
  "name": "babel-plate",
  "version": "0.0.1",
  "description": "Boilerplate for a new project: Webpack, Babel6, Mocha/Chai, eslint semistandard",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src test",
    "lint:fix": "./node_modules/.bin/eslint src test --fix",
    "build": "npm run build:prod",
    "build:dev": "echo \"You should be building with webpack-dev-server, use \\\"npm run start:dev\\\"\"",
    "build:prod": "export NODE_ENV=production; webpack -p --config webpack/webpack.config.prod.js",
    "clean:build": "rimraf ./lib/",
    "clean:modules": "rimraf ./node_modules/",
    "start": "npm run start:dev",
    "start:dev": "export NODE_ENV=development; node ./serving/server.js",
    "start:prod": "export NODE_ENV=production; node ./serving/server.js",
    "test": "mocha --compilers js:./test/compiler.js ./test/*.spec.js",
    "prepublish": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pl12133/babel-plate.git"
  },
  "keywords": [],
  "author": "pl12133",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.5.1",
    "eslint-config-semistandard": "^6.0.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^1.3.2",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "react-transform": "0.0.3",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.2",
    "svg-inline-loader": "^0.4.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "citysdk": "^1.0.1",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "es6-promise": "^3.1.2",
    "eslint-plugin-react": "^4.2.3",
    "exports-loader": "^0.6.3",
    "history": "^2.0.1",
    "imports-loader": "^0.6.5",
    "lodash": "^4.11.0",
    "lru-memoize": "^1.0.1",
    "node-sass": "^3.4.2",
    "rd3": "^0.5.3",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-bootstrap": "^0.28.4",
    "react-dom": "^0.14.7",
    "react-overlays": "^0.6.3",
    "react-router": "^2.0.1",
    "react-router-bootstrap": "^0.20.1",
    "sass": "^0.5.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "svg-inline-react": "^1.0.1",
    "whatwg-fetch": "^0.11.0"
  }
}
